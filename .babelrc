{
    "presets": [
      "react",
      [
        "env", {
          "targets": {
            "chrome": 59
          },
          "debug": true,
          "useBuiltIns": true,
          "modules": false,
          "exclude": [
            "es7.string.pad-start",
            "es7.string.pad-end",
            "web.timers",
            "web.immediate",
            "web.dom.iterable"
          ]
        }
      ]
    ],
    "plugins": ["transform-object-rest-spread"]
}
