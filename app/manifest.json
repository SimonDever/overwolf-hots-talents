{
  "manifest_version": 1,
  "type": "WebApp",
  "meta":{
    "name": "Heroes of the Storm Talent Picks",
    "author": "David Filipidisz",    
    "version": "1.0.0",
    "minimum-overwolf-version": "0.82.0",
    "access-name": "com.davidf.hotstalents",
    "description": "Check out Heroes of the Storm talent picks for every Hero, ordered by popularity or win rate.",
    "dock_button_title": "HoTS Talent Picks",
    "icon": "IconMouseOver.png",
    "icon_gray": "IconMouseNormal.png"
  },
  "data": {
    "start_window": "MainWindow",
    "externally_connectable": {
      "matches": [
        "https://raw.githubusercontent.com/*"
      ]
    },
    "windows": {
      "MainWindow": {
        "file": "Files/index.html",
        "transparent": true,
        "resizable": true,
        "size": {"width":400, "height":500},
        "min_size": {"width":400, "height":600},
        "max_size": {"width":600, "height":1000}
      }
    }
  }
}